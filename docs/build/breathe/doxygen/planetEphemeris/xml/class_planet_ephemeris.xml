      <memberdef kind="function" id="class_planet_ephemeris_1ae122d058133cbc58919c4f0160804e37" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>void</type>
        <definition>void PlanetEphemeris::UpdateState</definition>
